<form class="text-end" method="post" action="{{ path('app_admin_job_delete', {'id': job.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ job.id) }}">
    <button class="btn btn-danger m-3"><i class="fa-solid fa-trash-can" style="color: #ffffff;"></i></button>
</form>
